#! /bin/bash

# script to get downsampled results from already obtained results file

result_file_dir=$1
usage_vector_file=$2

paste $usage_vector_file $result_file_dir/results | grep -v '^0' | cut -f2- > $result_file_dir/results.ds 
paste $usage_vector_file $result_file_dir/results | grep '^0' | cut -f2- > $result_file_dir/results.ds.no_child_speech 
